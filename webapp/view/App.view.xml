<mvc:View controllerName="App.dataSelect.controller.App" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:utl="App.dataSelect.util"
	displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<Panel id="idPanel">
						<content>
							<Label text="Range date" labelFor="rangeDate"/>
							<utl:DualCalendar id="rangeDate" startDate="{/startDate}" endDate="{/endDate}"/>
						</content>
					</Panel>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>