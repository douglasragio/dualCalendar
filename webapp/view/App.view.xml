<mvc:View controllerName="App.dataSelect.controller.App" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Label text="Product" labelFor="productInput"/>
						<Input id="productInput" type="Text" textFormatMode="KeyValue" placeholder="Enter Product ..." showSuggestion="true" showValueHelp="true"
							valueHelpRequest=".onValueRequest"></Input>
						<DateRangeSelection id="fltrIssuanceDate" displayFormat="short" valueFormat="yyyy-MM-ddT00:00:00" change="handleChange" enabled="false"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>